{% extends 'AppBundle:public:public_base.html.twig' %}


{% block content %}
<div class="container">
  {# Previous and next links #}

  <div class="pure-g">
    <div class="pure-u-1-5">

      {% if previous is not null %}<a href="{{path('view_post_by_id', {'id': previous.id})}}"><h4>&#12296; Previous Post</h4></a>{% endif %}
    </div>

    <div class="pure-u-3-5">
      &nbsp;
    </div>

    <div class="pure-u-1-5">
      {% if next is not null %}<a href="{{path('view_post_by_id', {'id': next.id})}}"><h4>Next Post &#12297;</h4></a>{% endif %}
    </div>
  </div>

<div class="pure-g">

  <div class="pure-u-1-5">

  </div>
  <div class="pure-u-3-5">

    {% include 'AppBundle:post:post_master.html.twig' %}
  </div>
</div>



{# Post content #}
  <div class="row">
    <div class="col s12 m10 offset-m1">
    </div>
  </div>
</div>

{% endblock content %}
