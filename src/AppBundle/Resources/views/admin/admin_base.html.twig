{% extends 'AppBundle::base.html.twig' %}

{% block css %}
<link rel="stylesheet" href="{{asset('css/admin.css')}}">
{% endblock css %}

{% block body %}
  {# Holds the navigation bar #}
  {% block navbar %}
    {% include 'AppBundle:nav:admin_navbar.html.twig'  %}
  {% endblock navbar %}


  {# Block designed to be overridden to provide the page's content space. Note: you still need a <div class=container> #}
  <div class="container">
    <div class="row">
      <div class="col s12 m11 offset-m1">
        {% block content %}
        {% endblock content %}

      </div>
    </div>
  </div>

{% endblock body %}
